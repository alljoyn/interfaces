<node xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://www.allseenalliance.org/schemas/introspect.xsd" >
  <interface name="org.alljoyn.Notification.Producer">
    <annotation name="org.alljoyn.Bus.Secure" value="off"/>
    <description language="en">This interface defines the Dismiss
    method call.</description>

    <property name="Version" type="q" access="read">
      <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
      <description language="en">The interface version number.</description>
    </property>

    <method name="Dismiss">
      <description language="en">The Dismiss method call.</description>
      <arg name="msgId" type="i" direction="in"/>
    </method>
  </interface>
</node>
