<node xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://www.allseenalliance.org/schemas/introspect.xsd" >
  <interface name="org.alljoyn.Notification.Dismisser">
    <annotation name="org.alljoyn.Bus.Secure" value="false"/>
    <description language="en">This interface defines the Dismisser
    signal that is part of the Notification functionality.</description>

    <property name="Version" type="q" access="read">
      <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="false"/>
      <description language="en">The interface version number.</description>
    </property>

    <signal name="Dismiss" sessionless='true'>
      <description language="en">The Dismiss signal.</description>
      <arg name="msgId" type="i" direction="in"/>
      <arg name="appId" type="ay" direction="in"/>
    </signal>
  </interface>
</node>
